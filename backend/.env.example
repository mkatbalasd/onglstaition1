# Example environment configuration for the backend
#
# DB_HOST: address of the MySQL server
DB_HOST=localhost

# DB_USER: MySQL username
DB_USER=root

# DB_PASSWORD: MySQL password
DB_PASSWORD=

# DB_NAME: database to connect to
# Use the name defined in the SQL schema (see database/schema.sql)
DB_NAME=card_management

# PORT: port for the Express server
PORT=3000
